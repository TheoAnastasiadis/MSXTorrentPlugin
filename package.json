        "prettier": "2.8.7",
        "style-loader": "^3.3.2",
        "webpack": "^5.76.3",
        "webpack-cli": "^5.0.1",
        "workbox-webpack-plugin": "^6.5.4"
    },
    "dependencies": {
        "lodash": "^4.17.21",
