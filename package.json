{
    "name": "msxtorrentplugin",
    "version": "0.0.1",
    "description": "MSX plugin to play torrent videos",
    "private": true,
    "type": "module",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "webpack",
        "auto-build": "nodemon -w ./src --exec \"npm run build\"",
